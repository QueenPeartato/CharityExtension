{
  "manifest_version": 2,

  "name": "Cares Not Shares",
  "description": "If you can't change it, empower those who can.",
  "version": "1.0",

  "permissions": [
    "https://www.facebook.com/",
    "http://www.facebook.com/",
    "tabs", "http://*/*", "https://*/*"
  ],

  "icons": {
    "128": "image/icon.png"
  },

  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "image/icon.png",
      "default_popup": "popup.html"
  }
}
